{
  "kind": "collectionType",
  "collectionName": "board_games",
  "info": {
    "singularName": "board-game",
    "pluralName": "board-games",
    "displayName": "Board Game"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "gameName": {
      "type": "string",
      "required": true
    },
    "startDate": {
      "type": "date"
    },
    "duration": {
      "type": "decimal"
    },
    "expansion": {
      "type": "string"
    },
    "mission": {
      "type": "string"
    },
    "characters": {
      "type": "string"
    },
    "players": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::player.player"
    },
    "extraPlayer": {
      "type": "string",
      "default": "NA"
    },
    "success": {
      "type": "boolean"
    },
    "notes": {
      "type": "text"
    }
  }
}
